{
  "name": "rekit-studio",
  "version": "3.0.0",
  "description": "Web IDE for Rekit projects.",
  "keywords": [
    "rekit",
    "react",
    "redux",
    "react-router"
  ],
  "main": "./lib/index.js",
  "bin": {
    "rekit-studio": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/supnate/rekit.git"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "syntax-dynamic-import",
      "lodash",
      [
        "module-resolver",
        {
          "alias": {
            "src": "./src"
          }
        }
      ]
    ]
  },
  "nyc": {
    "include": [
      "index.js",
      "src/**/*.js",
      "tools/lib/*.js"
    ],
    "exclude": [
      "tools/**/templates",
      ".tmp"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": false,
    "sourceMap": false,
    "instrument": true
  },
  "rekit": {
    "version": "2.0.3",
    "devPort": 6078,
    "buildPort": 6079,
    "appType": "rekit-react",
    "plugins": [],
    "folders": [
      "build",
      "public",
      "bin",
      "scripts",
      "config",
      "dll",
      "lib"
    ],
    "files": [
      "package.json"
    ],
    "css": "less"
  },
  "license": "MIT",
  "scripts": {
    "start": "node ./scripts/start.js",
    "serveBuild": "node ./bin/index.js",
    "test2": "node -v",
    "build": "node ./scripts/build.js",
    "npm:publish": "node ./tools/npm_publish.js",
    "test": "node ./tools/run_test.js",
    "build:test": "node ./tools/server.js -m build"
  },
  "files": [
    "bin",
    "build",
    "lib",
    "src"
  ],
  "dependencies": {
    "argparse": "^1.0.10",
    "babel-traverse": "^6.23.1",
    "body-parser": "^1.18.2",
    "chalk": "1.1.3",
    "compression": "^1.7.3",
    "express": "^4.0.1",
    "express-history-api-fallback": "^2.2.1",
    "express-ws": "^4.0.0",
    "import-from": "^2.1.0",
    "lodash": "^4.17.10",
    "node-pty": "^0.7.6",
    "package-json": "^4.0.1",
    "prettier": "^1.14.2",
    "rekit-core": "^2.0.0",
    "shelljs": "^0.8.2",
    "socket.io": "^2.0.1",
    "watchpack": "^1.3.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.3",
    "ansi-to-html": "^0.6.6",
    "antd": "^3.9.2",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "0.28.7",
    "d3": "^5.6.0",
    "d3-tip": "^0.9.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "fuzzysort": "^1.1.4",
    "html-webpack-plugin": "2.29.0",
    "immutability-helper": "^2.7.1",
    "jest": "20.0.4",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "monaco-editor": "^0.14.3",
    "monaco-editor-webpack-plugin": "^1.5.1",
    "nock": "^9.2.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.5.0",
    "react-beautiful-dnd": "^9.0.1",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.5.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-split-pane": "2.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "style-loader": "0.19.0",
    "svg-sprite-loader": "^3.9.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "tinygradient": "^0.4.0",
    "url-loader": "0.6.2",
    "virtual-module-webpack-plugin": "^0.4.0",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3",
    "worker-loader": "^2.0.0",
    "xterm": "^3.10.1"
  }
}
